---
import { supabase } from "../lib/supabase";

// Fetch skills data
const { data: skills, error } = await supabase
  .from("skills")
  .select("*")
  .limit(1)
  .maybeSingle();
if (error) {
  console.error("Error fetching skills:", error.message);
}
---

<section id="about" class="py-20 bg-gray-800 overflow-hidden">
  <div class="container mx-auto px-4 sm:px-6 w-full">
    <div
      class="grid md:grid-cols-3 gap-10 items-center border border-green-500 rounded-lg p-8 transition-all duration-300 hover:border-orange-500 hover:shadow-orange-500/50 hover:shadow-lg"
    >
      {/* Bagian Kiri - BIOGRAPHY */}
      <div
        class="md:col-span-1 border-r-2 border-green-500 pr-8 opacity-0 translate-x-[-50px] animate-slideInLeft"
      >
        <h2
          class="text-3xl sm:text-4xl font-bold mb-6 text-orange-500 sm:text-left animate-pulse"
        >
          BIOGRAPHY
        </h2>
        <p class="text-gray-300 sm:text-left leading-relaxed">
          {skills.biography}
        </p>
      </div>

      {/* Bagian Tengah - FOTO */}
      <div
        class="md:col-span-1 flex justify-center border-r-2 border-green-500 px-8 opacity-0 scale-90 animate-fadeIn"
      >
        <img
          src={skills.avatar_url}
          alt="Profil"
          class="rounded-lg shadow-xl w-full max-w-xs sm:max-w-md transition-transform duration-500 hover:scale-105"
        />
      </div>

      {/* Bagian Kanan - Skill & Interest */}
      <div
        class="md:col-span-1 pl-8 opacity-0 translate-x-[50px] animate-slideInRight"
      >
        <h3
          class="text-xl sm:text-2xl font-semibold mb-4 sm:text-left text-white"
        >
          Skill & Interest
        </h3>
        <ul class="list-disc list-inside text-gray-300 sm:text-left space-y-1">
          {
            skills.skills ? (
              skills.skills.split(", ").map((skill, index) => (
                <li
                  key={index}
                  class="text-white transition-all duration-300 hover:text-orange-400 cursor-default  "
                >
                  {skill}
                </li>
              ))
            ) : (
              <li class="text-gray-400">Data skill belum tersedia.</li>
            )
          }
        </ul>

        {/* Bagian Tools */}
        <h3
          class="text-xl sm:text-2xl font-semibold mt-6 sm:text-left text-white"
        >
          Tools
